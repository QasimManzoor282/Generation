<header>
    <h1>Test</h1>
    <ul>
        {% for link in linklists.main-menu.links -%}
            <li>{{ link.title | link_to: link.url }}</li>
        {%- endfor %}
    </ul>
    {% if settings.show_search %} 
        <form action="/search">
            <input type="text" name="q">
            <button type="submit">Search</button>
        </form>
    {% endif %}
</header>
